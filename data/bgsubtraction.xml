<Blobserver>
    <Flow>
        <Detector>
            <Type>Detector_BgSubtractor</Type>
            <Param>
                <Name>verbose</Name>
                <Value>0</Value>
            </Param>
            <Param>
                <Name>filterSize</Name>
                <Value>3</Value>
            </Param>
            <Param>
                <Name>learningTime</Name>
                <Value>300</Value>
            </Param>
        </Detector>
        <Source>
            <Type>Source_2D_OpenCV</Type>
            <Subsource>300</Subsource>
            <Param>
                <Name>framerate</Name>
                <Value>30</Value>
            </Param>
            <Param>
                <Name>iso</Name>
                <Value>400</Value>
            </Param>
            <Param>
                <Name>width</Name>
                <Value>640</Value>
            </Param>
            <Param>
                <Name>height</Name>
                <Value>480</Value>
            </Param>
            <Param>
                <Name>exposureTime</Name>
                <Value>500</Value>
            </Param>
            <Param>
                <Name>gain</Name>
                <Value>512</Value>
            </Param>
            <Param>
                <Name>gamma</Name>
                <Value>1.8</Value>
            </Param>
            <Param>
                <Name>whiteBalanceRed</Name>
                <Value>512</Value>
            </Param>
            <Param>
                <Name>whiteBalanceBlue</Name>
                <Value>512</Value>
            </Param>
            <Param>
                <Name>save</Name>
                <Value>0 5 /home/manu/tmp/test_</Value>
            </Param>
        </Source>
        <Client>
            <Address>localhost</Address>
            <Port>9000</Port>
        </Client>
        <Server>
            <Address>localhost</Address>
            <Port>9002</Port>
        </Server>
    </Flow>
</Blobserver>
